#!/bin/bash

export SERVICE_NAME=gitlab-runner

export SERVICE_GITLAB_RUNNER_IMAGE_VERSION=v1.0.1

export SERVICE_GITLAB_URL=${SERVICE_GITLAB_URL:-"http://gitlab"}

export SERVICE_GITLAB_RUNNER_COMPANY=${SERVICE_GITLAB_RUNNER_COMPANY:-zmicro}

export SERVICE_GITLAB_RUNNER_TOKEN=${SERVICE_GITLAB_RUNNER_TOKEN}
export SERVICE_GITLAB_RUNNER_EXECUTOR=${SERVICE_GITLAB_RUNNER_EXECUTOR}
export SERVICE_GITLAB_RUNNER_DOCKER_IMAGE=${SERVICE_GITLAB_RUNNER_IMAGE}
export SERVICE_GITLAB_RUNNER_DESCRIPTION=${SERVICE_GITLAB_RUNNER_DESCRIPTION}
export SERVICE_GITLAB_RUNNER_DOCKER_NETWORK=${SERVICE_GITLAB_RUNNER_DOCKER_NETWORK:-"compose-ingress"}